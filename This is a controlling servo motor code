// C++ code
//
#include <Servo.h>

Servo servo_2;
Servo servo_3;
Servo servo_4;
Servo servo_5;
Servo servo_6;

void setup()
{
  servo_2.attach(2, 500, 2500);
  servo_3.attach(3, 500, 2500);
  servo_4.attach(4, 500, 2500);
  servo_5.attach(5, 500, 2500);
  servo_6.attach(6, 500, 2500);


}

void loop()
{
    servo_2.write(90);
     delay(1000);// Delay a little bit to improve simulation performance
    servo_2.write(-90);
   
    servo_3.write(90);
    delay(1000); // Delay a little bit to improve simulation performance
    servo_3.write(-90);
  
   servo_4.write(90);
    delay(1000); // Delay a little bit to improve simulation performance
    servo_4.write(-90);
   
   servo_5.write(90);
    delay(1000); // Delay a little bit to improve simulation performance
    servo_5.write(-90);
    
   servo_6.write(90);
    delay(1000); // Delay a little bit to improve simulation performance
    servo_6.write(-90);
    
}
